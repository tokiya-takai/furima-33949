<li class='list'>
  <%= link_to "#" do %>
  <div class='item-img-content'>
    <%= image_tag item.image, class: "item-img" %>
    <%# <div class='sold-out'>
      <span>Sold Out!!</span>
    </div> %>
  </div>
  <div class='item-info'>
    <h3 class='item-name'>
      <%= item.title %>
    </h3>
    <div class='item-price'>
      <span><%= item.price %>å††<br><%= item.delivery.name %></span>
       <div class='star-btn'>
        <%= image_tag "star.png", class:"star-icon" %>
        <span class='star-count'>0</span>
      </div>
    </div>
  </div>
  <% end %>
</li>